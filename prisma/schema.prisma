generator client {
  provider = "prisma-client"
}

datasource db {
  provider = "mongodb"
}

model User {
  id String @id @map("_id") @db.ObjectId @default(auto())
  
  nome String
  dtNascimento DateTime?
  email String @unique
  senha Int

  dtCriacao DateTime @default(now())
  dtAtualizacao DateTime @updatedAt
}